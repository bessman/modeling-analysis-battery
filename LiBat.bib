	pages = {76--88},
	file = {IEEE Xplore Abstract Record:/home/alexander/.mozilla/firefox/odav7jb2.default/zotero/storage/QMWATWSF/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/home/alexander/.mozilla/firefox/odav7jb2.default/zotero/storage/HMRGRXVZ/Cheng et al. - 2011 - Battery-Management System (BMS) and SOC Developmen.pdf:application/pdf}
