@article{lee_surface_2001,
	title = {Surface structure of lithiated graphite by X-ray photoelectron diffraction},
	volume = {477},
	issn = {0039-6028},
	url = {http://www.sciencedirect.com/science/article/pii/S0039602800011067},
	doi = {10.1016/S0039-6028(00)01106-7},
	abstract = {The surface composition and structure of a stage-one lithium intercalation compound (Li-{GIC)} was studied by X-ray photoelectron diffraction ({XPD)} from Li 1s and C 1s core levels. The Li-{GIC} was prepared in situ by vapor phase intercalation of lithium into highly oriented pyrolitic graphite ({HOPG)} in an ultra-high vacuum system. Stage-one Li-{GIC}, {LiC6}, having a characteristic golden color, was obtained using Li evaporation onto a {HOPG} substrate at 400 K, while a metallic lithium overlayer was observed at depositions below room temperature. {XPD} patterns of Li 1s and C 1s intensities as a function of the polar emission angle were obtained at a fixed photon energy of 1253.6 {eV.} The experimental {XPD} patterns on the {HOPG} and the stage-one Li-{GIC} were both in quite good agreement with calculated {XPD} patterns based on {Rehr–Albers} separable representation for scattering of the emitted photoelectrons. The structural model for the surface of the Li-{GIC} producing the best fit to the experimental data has the bulk {LiC6} lattice with surface termination in the graphene plane.},
	number = {2–3},
	urldate = {2014-04-08},
	journal = {Surface Science},
	author = {Lee, Choong Man and Yang, S. -H and Mun, B. -J and Ross Jr., Philip N},
	month = apr,
	year = {2001},
	keywords = {Alkali metals, Graphite, Photoelectron diffraction measurement, Surface structure, morphology, roughness, and topography},
	pages = {126--132},
	file = {ScienceDirect Full Text PDF:/home/alexander/.mozilla/firefox/odav7jb2.default/zotero/storage/F37D7SK4/Lee et al. - 2001 - Surface structure of lithiated graphite by X-ray p.pdf:application/pdf;ScienceDirect Snapshot:/home/alexander/.mozilla/firefox/odav7jb2.default/zotero/storage/3HKEFZZT/S0039602800011067.html:text/html}
}

