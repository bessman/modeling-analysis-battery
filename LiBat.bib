@article{zavalis_aging_2013,
	title = {Aging in lithium-ion batteries: Model and experimental investigation of harvested {LiFePO4} and mesocarbon microbead graphite electrodes},
	volume = {110},
	issn = {0013-4686},
	shorttitle = {Aging in lithium-ion batteries},
	url = {http://www.sciencedirect.com/science/article/pii/S0013468613010049},
	doi = {10.1016/j.electacta.2013.05.081},
	abstract = {This study investigates aging in {LiFePO4/mesocarbon} microbead graphite cells that have been subjected to either a synthetic hybrid drive cycle or calendar aging, at 22 {°C.} The investigation involves detailed examination and comparison of harvested fresh and aged electrodes. The electrode properties are determined using a physics-based electrochemical impedance spectroscopy ({EIS)} model that is fitted to three-electrode {EIS} measurements, with input from measured electrode capacity and scanning electrode microscopy ({SEM).} Results from the model fitting provide a detailed insight to the electrode degradation and is put into context with the behavior of the full cell aging. It was established that calendar aging has negligible effect on cell impedance, while cycle aging increases the impedance mainly due to structural changes in the {LiFePO4} porous electrode and electrolyte decomposition products on both electrodes. Further, full-cell capacity fade is mainly a consequence of cyclable lithium loss caused by electrolyte decomposition.},
	urldate = {2014-01-07},
	journal = {Electrochimica Acta},
	author = {Zavalis, Tommy Georgios and Klett, Matilda and Kjell, Maria H. and Behm, Mårten and Lindström, Rakel Wreland and Lindbergh, Göran},
	month = nov,
	year = {2013},
	keywords = {Aging, {EIS} modeling, Graphite, {LiFePO4}, Lithium-ion battery},
	pages = {335--348},
	file = {Zavalis et al. - 2013 - Aging in lithium-ion batteries Model and experime.html:/home/alexander/.mozilla/firefox/odav7jb2.default/zotero/storage/NDJUSEG9/Zavalis et al. - 2013 - Aging in lithium-ion batteries Model and experime.html:text/html;Zavalis et al. - 2013 - Aging in lithium-ion batteries Model and experime.pdf:/home/alexander/.mozilla/firefox/odav7jb2.default/zotero/storage/E79WBEMT/Zavalis et al. - 2013 - Aging in lithium-ion batteries Model and experime.pdf:application/pdf}
